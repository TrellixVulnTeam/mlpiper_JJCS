{
    "name": "1P KMeans High-Level Prediction",
    "engineType": "SageMaker",
    "pipe": [
        {
            "name": "AWS S3 file uploader (Test)",
            "id": 1,
            "type": "aws_s3_file_uploader_test",
            "parents": [],
            "arguments": {
                "local_filepath": "/tmp/sagemaker_mnist_test_dataset.csv",
                "skip_uploading": "false"
            }
        },
        {
            "name": "SageMaker KMeans Batch Predictor (Test)",
            "id": 2,
            "type": "sagemaker_kmeans_batch_predictor_test",
            "parents": [
                {
                    "parent": 1,
                    "output": 0
                }
            ],
            "arguments": {
                "downloaded_results_filepath": "/tmp/sagemaker_mnist_test_dataset.out",
                "skip_s3_model_uploading": "false"
            }
        }
    ],
    "systemConfig": {
        "modelFileSourcePath": "__PLACEHOLDER__"
    },
    "executionEnvironment": {
        "configs": {
            "engConfig": {
                "type": "sagemaker",
                "arguments": {
                    "region": {
                        "value": "us-west-2",
                        "type": "string",
                        "optional": false,
                        "label": "Region",
                        "description": "The AWS Region to send the request to",
                        "editable": true
                    },
                    "aws_access_key_id": {
                        "value": "",
                        "type": "string",
                        "optional": false,
                        "label": "Access Key ID",
                        "description": "A long term credential access key ID",
                        "editable": true
                    },
                    "aws_secret_access_key": {
                        "value": "",
                        "type": "string",
                        "optional": false,
                        "label": "Secret Access Key",
                        "description": "A long term credential secret access key",
                        "editable": true
                    },
                    "iam_role": {
                        "value": "",
                        "default_value": "",
                        "type": "string",
                        "optional": false,
                        "label": "IAM Role",
                        "description": "An IAM identity that has specific permissions",
                        "editable": true
                    }
                }
            }
        }
    }
}
